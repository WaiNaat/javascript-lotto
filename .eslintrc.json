{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["warn", "windows"],
        "no-console": "off",
        "no-param-reassign": ["warn", { "props": false }],
        "import/extensions": ["error", "always"],
        "class-methods-use-this": "off"
    }
}
